{
  "name": "nextjs-blacklist",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed": "node prisma/seed.js",
    "prepare": "husky install",
    "format": "prettier --write '**/*{js,jsx,ts,tsx,json,css,scss,md}' --list-different",
    "format:staged": "pretty-quick --staged",
    "eslint": "node --max-old-space-size=3072 ./node_modules/eslint/bin/eslint.js --ext .ts,.tsx,.js,.jsx,.json ."
  },
  "engines": {
    "node": "16.16.0"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "1.0.4",
    "@prisma/client": "4.1.1",
    "husky": "8.0.1",
    "next": "12.2.3",
    "next-auth": "4.10.3",
    "prettier": "2.7.1",
    "pretty-quick": "^3.1.3",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.54.0"
  },
  "devDependencies": {
    "eslint": "8.23.1",
    "eslint-config-next": "12.2.3",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "prisma": "4.1.1"
  }
}
